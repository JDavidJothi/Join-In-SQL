SELECT * FROM Customers ORDER BY Country ASC, CustomerName DESC;

CREATE TABLE tblEmployee(ID INT,Name VARCHAR(250),Gender VARCHAR(250),Salary INT,DepartmentId INT FOREIGN KEY REFERENCES tblDepartment(Id));

CREATE TABLE tblDepartment(Id INT NOT NULL Primary Key,Department_Name VARCHAR(250),Location VARCHAR(250),DepartmentHead VARCHAR(250));

DROP TABLE tblDepartment; 
DROP TABLE tblEmployee; 

